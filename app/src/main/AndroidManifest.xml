<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.svb.Fooddelivery" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.svb.Fooddelivery.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.svb.Fooddelivery.orderdetails"
            android:label="@string/title_activity_order_detail" >
        </activity>
        <activity
            android:name="com.example.svb.Fooddelivery.amole"
            android:label="@string/title_activity_amole" >
        </activity>
    </application>

</manifest>
